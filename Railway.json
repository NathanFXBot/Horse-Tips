{
  "build": {
    "builder": "NIXPACKS",
    "nixpacksPlan": {
      "phases": {
        "setup": {
          "nixPkgs": ["nodejs", "python3"]
        },
        "install": {
          "cmds": ["npm install"]
        },
        "start": {
          "cmd": "node index.js"
        }
      }
    }
  }
}
